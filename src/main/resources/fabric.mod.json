    "BluSpring"
  ],
  "contact": {
    "repo": "https://github.com/KiltMC/Kilt",
    "issues": "https://github.com/KiltMC/Kilt/issues"
  },
  "license": "LGPL-2.1-only",
  "icon": "assets/kilt/icon.png",
  "environment": "*",

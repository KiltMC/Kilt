    "client": [
      "xyz.bluspring.kilt.client.KiltClient"
    ],
    "main": [
      "xyz.bluspring.kilt.Kilt"
    ],
    "preLaunch": [
      "xyz.bluspring.kilt.loader.KiltEarlyInitializer"
    ],
    "server": [
      "xyz.bluspring.kilt.server.dedicated.KiltDedicatedServer"
    ],
    "mm:early_risers": [
